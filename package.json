    "@bubblewrap/cli": "1.17.2",
    "@types/babel__core": "^7",
    "@types/eslint": "8.4.3",
    "@types/web": "0.0.68",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "8.17.0",
    "eslint-plugin-jsdoc": "39.3.2",
    "lint-staged": "13.0.1",
    "node-scripts-docs": "0.0.14",
    "prettier": "2.6.2",
